@{
    ViewData["Title"] = "Home Page";
    Layout = null;
}
<html lang="en">
<head>
    <title>模型预览</title>
    <link rel="stylesheet" href="~/iView/iview.css">
    <link href="~/element-ui/index.css" rel="stylesheet" />
    <link href="~/css/task_index.css" rel="stylesheet" />
    <link href="~/css/mycss.css" rel="stylesheet" />
    <script src="~/vue/vue.js"></script>
    <script src="~/element-ui/index.js"></script>
    <script src="~/vue/axios.min.js"></script>
    <script src="~/iView/iview.min.js"></script>
    
    <style>
        [v-cloak] {
            display: none;
        }
    </style>
</head>
<body>

    <div id="header-container" class="task-header" v-cloak>
        <div class="img">  <img style="margin-left:-40px;" src="~/images/bimface.png" /></div>
        <div class="txt">
            BIMFACE DEMO 示例
        </div>
        <div class="fun">
            <ul>
                <li v-for="(item,index) in links" v-bind:class="{actived:activedIndex==index}"> <a v-bind:href="item.url">{{item.text}}</a></li>
            </ul>
        </div>
    </div>
    @RenderBody()
</body>
</html>
<script>
    var $headerVue = new Vue({
        el: "#header-container",
        data: {
            activedIndex: -1,
            links: [{ text: '后台任务', url: '/' }, { text: '指南', url: '/Home/Guide' }, { text: '帮助', url: '/Home/Help' }, { text: '安全退出', url: '/Home/SignOut' }]
        }, methods: {
        }, created: function () {
        }
    });
</script>
